        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase",
        "mojopay"
    ],
    "homepage": "https://github.com/knightar/omnipay-mojopay",
    "license": "MIT",
    "authors": [
        {
            "name": "Brandon Lis",
            "email": "brandon@artician.net",
            "homepage": "http://artician.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "omnipay/common": "~2.0"
    },
    "require-dev": {
        "omnipay/tests": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\Mojopay\\": "src"
        }
